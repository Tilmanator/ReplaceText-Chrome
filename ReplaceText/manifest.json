{
  "manifest_version": 2,
  "name": "Replace Text",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.0.0.js","content.js","options.js"]
  }
],
"browser_action": {
  "default_icon": "icon.png"
},
"background": {
  "scripts": ["background.js"]
},

"permissions":  ["storage"],
"options_page": "options.html"

}

